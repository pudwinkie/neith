<script language="javascript">
    function runcmd(s) {
        var sh = new ActiveXObject("WScript.Shell");
        var path = document.location.pathname;
        path = path.substr(0, path.lastIndexOf("\\")+1);
        var fn = ('"' + path + s + '"').replace(/%20/g," ");
        sh.run(fn);
        return false;
    }

    function runeither(vb, cs) {
        var e = window.event;
        var d = document.getElementById("popup");
        d.style.backgroundColor = "white";
        var top = Math.max(document.documentElement.scrollTop, document.body.scrollTop);
        var left = Math.max(document.documentElement.scrollLeft, document.body.scrollLeft);
        d.style.left = (e.clientX - 4 + left) + "px";
        d.style.top = (e.clientY - 4 + top) + "px";
        var d1 = document.getElementById("popup1");
        d1.onclick = function () { d.style.backgroundColor = "gray";  return runcmd(cs); };
        var d2 = document.getElementById("popup2");
        d2.onclick = function () { d.style.backgroundColor = "gray";  return runcmd(vb); };
        d.style.display = "block";
        return false;
    }
</script>
