<!-- 
digraph G {

	subgraph cluster_0 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=white];
		a0 -> a1 -> a2 -> a3;
		label = "process #1";
	}

	subgraph cluster_1 {
		node [style=filled];
		b0 -> b1 -> b2 -> b3;
		label = "process #2";
		color=blue
	}
	start -> a0;
	start -> b0;
	a1 -> b3;
	b2 -> a3;
	a3 -> a0;
	a3 -> end;
	b3 -> end;

	start [shape=Mdiamond];
	end [shape=Msquare];
}
-->

<digraph id="G">
  <subgraph id="cluster_0">
    <style>filled</style>
    <color>lightgrey</color>
    <styles>
      <node>
        <style>filled</style>
        <color>white</color>
      </node>
    </styles>
    <edge>
      <node id="a0" />
      <node id="a1" />
      <node id="a2" />
      <node id="a3" />
    </edge>
    <label>process #1</label>
  </subgraph>
  <subgraph id="cluster_1">
    <styles>
      <node>
        <style>filled</style>
      </node>
    </styles>
    <edge>
      <node id="b0" />
      <node id="b1" />
      <node id="b2" />
      <node id="b3" />
    </edge>
    <label>process #2</label>
  </subgraph>
  <edge>
    <node id="start" />
    <node id="a0" />
  </edge>
  <edge>
    <node id="start" />
    <node id="b0" />
  </edge>
  <edge>
    <node id="a1" />
    <node id="b3" />
  </edge>
  <edge>
    <node id="b2" />
    <node id="a3" />
  </edge>
  <edge>
    <node id="a3" />
    <node id="a0" />
  </edge>
  <edge>
    <node id="a3" />
    <node id="end" />
  </edge>
  <edge>
    <node id="b3" />
    <node id="end" />
  </edge>
  <node id="start">
    <shape>Mdiamond</shape>
  </node>
  <node id="end">
    <shape>Msquare</shape>
  </node>
</digraph>