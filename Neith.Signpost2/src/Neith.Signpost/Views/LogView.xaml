<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:Neith.Signpost"
	x:Class="Neith.Signpost.LogView"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:LogViewModel x:Key="LogViewModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource LogViewModelDataSource}}"/>
</UserControl>
