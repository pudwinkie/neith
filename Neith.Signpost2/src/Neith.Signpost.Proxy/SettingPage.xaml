<UserControl x:Class="Neith.Signpost.Proxy.SettingPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:p="clr-namespace:Neith.Signpost.Proxy.Properties" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
  <Grid DataContext="{Binding {x:Static p:Settings.Default}}">
    <Label Content="サーバアドレス" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" VerticalAlignment="Top" />
    <TextBlock Height="23" HorizontalAlignment="Left" Margin="112,13,0,0" Text="{Binding ChannelName}" VerticalAlignment="Top" Width="160" />
  </Grid>
</UserControl>
